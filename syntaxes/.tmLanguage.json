{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Tilt",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"match": "\\b(k8s_resource|enable_feature|default_registry|fall_back_on|k8s_resource_assembly_version|run|helm|sync|trigger|restart_container|local_git_repo)\\b",
					"captures": {
						"0": {
							"name": "variable.language"
						}
					}
				},
				{
					"match": "\\b(allow_k8s_contexts|analytics_settings|blob|custom_build|dc_resource|decode_json|decode_yaml|decode_yaml_stream|default_registry|disable_snapshots|docker_build|docker_compose|docker_prune_settings|enable_feature|encode_json|encode_yaml|encode_yaml_stream|fail|fall_back_on|filter_yaml|helm|include|k8s_context|k8s_kind|k8s_resource|k8s_yaml|kustomize|listdir|load|local|local_resource|read_file|read_json|read_yaml|read_yaml_stream|restart_container|run|secret_settings|set_team|struct|sync|trigger_mode|update_settings|version_settings|watch_file|workload_to_resource_function|os.getcwd|os.getenv|os.putenv|os.unsetenv|os.path.abspath|os.path.basename|os.path.dirname|os.path.exists|os.path.join|os.path.realpath|config.define_bool|config.define_string|config.define_string_list|config.parse|config.set_enabled_resources)\\b",
					"captures": {
						"0": {
							"name": "keyword.control"
						}
					}
				},
				{
					"match": "(=)\\s*",
					"captures": {
						"1": {
							"name": "keyword.operator.assignment.js"
						}
					}
				},
				{
					"match": "\\b(^[a-zA-Z]*)\\b",
					"captures": {
						"0": {
							"name": "variable.name"
						}
					}
				}
			]
		},
		"strings": {
			"name": "string.quoted",
			"begin": "'",
			"end": "'",
			"captures": {
				"0": {
					"name": "string.quoted.single"
				}
			},
			"patterns": [
				{
					"name": "constant.character.escape.osef",
					"match": "\\\\."
				}
			]
		},
		"comments": {
			"name": "comment",
			"begin": "#",
			"end": "\n"
		}
	},
	"scopeName": "source.Tiltfile"
}